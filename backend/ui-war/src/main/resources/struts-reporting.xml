<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!-- Configuration for the reporting package. -->
    <package name="reporting" extends="foros-default" abstract="true">
        <action name="cancel" class="com.foros.action.reporting.CancellablePageReportingAction" method="cancel"/>

        <action name="cancelAsync" class="com.foros.action.reporting.CancellablePageReportingAction" method="cancelAsync"/>

        <action name="run" class="com.foros.action.reporting.CancellablePageReportingAction" method="showCancellableView">
            <result name="success" type="tiles">reporting.cancellable</result>
        </action>
    </package>

    <!-- Referrer Report -->
    <package name="referrer" extends="reporting" abstract="true">
        <action name="options" class="com.foros.action.reporting.referrer.ViewReferrerReportingAction" method="view">
            <result name="success" type="tiles">reporting.Referrer.view</result>
        </action>

        <action name="cancellableRun" class="com.foros.action.reporting.referrer.RunReferrerReportingAction" method="execute">
            <interceptor-ref name="saveStack"/>
            <result name="success" >/reporting/referrer/referrerResult.jsp</result>
            <result name="input" >/reporting/referrer/referrerResult.jsp</result>
        </action>
    </package>
    <package name="referrer-admin" extends="referrer" namespace="/admin/reporting/referrer"/>
    <package name="referrer-publisher" extends="referrer" namespace="/publisher/reporting/referrer"/>

    <!-- Custom Report -->
    <package name="custom" extends="reporting" abstract="true">
        <action name="options" class="com.foros.action.reporting.custom.ViewOlapCustomReportingAction" method="view">
            <result name="success" type="tiles">reporting.Custom.view</result>
        </action>

        <action name="cancellableRun" class="com.foros.action.reporting.custom.RunOlapCustomReportingAction" method="execute">
            <interceptor-ref name="saveStack"/>
            <result name="success">/reporting/custom/olapCustomResult.jsp</result>
            <result name="detailsOnly">/reporting/custom/olapCustomDetails.jsp</result>
            <result name="input">/reporting/custom/olapCustomResult.jsp</result>
        </action>

    </package>
    <package name="custom-admin" extends="custom" namespace="/admin/reporting/custom"/>

