            <xml-property name="queryText"><![CDATA[
select 
/*replace it in beforeOpen*/ stimestamp,
wo.source,
wo.operation,
case user_status when 'U' then 'Undefined'
                 when 'O' then 'Opted Out' 
                 when 'I' then 'Opted In'
                 when 'P' then 'Probe'
                 when 'N' then 'no RTB ID'
 end user_status,
ct cohort,
curct current_cohort,
op.value os,
bp.value browser,
case result when 'S' then 'Success'
            when 'F' then 'Failure' 
            when 'U' then 'Undefined'
 end result,
count,
wo.app,
'1' site_name,
w.tag_id tag_id,
'1' cc_name,
w.cc_id cc_id
  from webstats w
       left join weboperation wo on w.web_operation_id = wo.web_operation_id
       left join userproperty bp on (bp.user_property_id = w.browser_property_id)
       left join userproperty op on (op.user_property_id = w.os_property_id),
       colocation c,
       (select ?::date as dsp_StartDate,
               ?::date as dsp_EndDate,
               ?::integer as dsp_ISP,
               ?::varchar as dsp_Application,
               ?::varchar as dsp_Test
       ) param
 where stimestamp >= param.dsp_StartDate and stimestamp < param.dsp_EndDate + 1
   and w.colo_id = c.colo_id
   and c.account_id = param.dsp_ISP
   and wo.app = param.dsp_Application
   and ( (param.dsp_Test = 'EXCL' and w.test = 'N')
         or (param.dsp_Test = 'INCL' and 1 = 1)
         or (param.dsp_Test = 'ONLY' and w.test = 'Y')
      )
]]></xml-property>
