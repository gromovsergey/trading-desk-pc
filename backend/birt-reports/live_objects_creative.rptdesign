            <xml-property name="queryText"><![CDATA[ select 
  a.agency_account_id, 
  a.account_id, 
  cr.creative_id,
  (select array_agg(cc.creative_category_id) from creativecategory_creative ccc join creativecategory cc on cc.creative_category_id = ccc.creative_category_id where ccc.creative_id = cr.creative_id and cc.cct_id = 0) visual_ids,
  (select array_agg(cc.creative_category_id) from creativecategory_creative ccc join creativecategory cc on cc.creative_category_id = ccc.creative_category_id where ccc.creative_id = cr.creative_id and cc.cct_id = 1) content_ids,
  st.name,
  cs.protocol_name,
  (select cov.value from creativeoptionvalue cov join options o on o.option_id = cov.option_id where cov.creative_id = cr.creative_id and o.token = 'DESTURL' order by cov.option_id limit 1) desturl,
  (select cov.value from creativeoptionvalue cov join options o on o.option_id = cov.option_id where cov.creative_id = cr.creative_id and o.token = 'CRCLICK' order by cov.option_id limit 1) crclick
from 
  account a join creative cr on cr.account_id = a.account_id
  join creativesize cs on cs.size_id = cr.size_id
  join sizetype st on st.size_type_id = cs.size_type_id
  left join account agency on a.agency_account_id = agency.account_id
where  
  (a.agency_account_id is not null or a.flags &amp; 1 = 0)
  and (a.status = 'A' or a.version > now() - interval '48 hours') 
  and (cr.status = 'A' or cr.version > now() - interval '48 hours') 
  and (a.agency_account_id is null or agency.flags &amp; 1 = 0 and (agency.status = 'A' or agency.version > now() - interval '48 hours')) 
  and a.country_code = ?]]></xml-property>
