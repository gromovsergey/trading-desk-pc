            <xml-property name="queryText"><![CDATA[select i.invoice_id
     ,(case when adv.agency_account_id is null then 'Advertiser' else 'Agency' end) account_type
      ,(case when adv.agency_account_id is null then adv.name else ag.name end) account_name
      ,u.first_name||' '||u.last_name bill_to_user
      ,nvl2(billing_adr.line1, billing_adr.line1||', ', billing_adr.line1)||nvl2(billing_adr.line2,billing_adr.line2||', ',billing_adr.line2)||nvl2(billing_adr.line3,billing_adr.line3||', ',billing_adr.line3)||nvl2(billing_adr.city,billing_adr.city||', ',billing_adr.city)||nvl2(billing_adr.state,billing_adr.state||', ',billing_adr.state)||billing_adr.zip bill_address
      ,nvl2(legal_adr.line1, legal_adr.line1||', ', legal_adr.line1)||nvl2(legal_adr.line2,legal_adr.line2||', ',legal_adr.line2)||nvl2(legal_adr.line3,legal_adr.line3||', ',legal_adr.line3)||nvl2(legal_adr.city,legal_adr.city||', ',legal_adr.city)||nvl2(legal_adr.state,legal_adr.state||', ',legal_adr.state)||legal_adr.zip legal_address
      ,case when adv.agency_account_id is null then adv.company_registration_number else ag.company_registration_number end cnpj
      ,i.invoice_date invoice_date
      ,i.campaign_id campaign_id
      ,i.campaign_name
      ,to_char(start_billing_period, 'dd/mm/yyyy')||' - '||to_char(end_billing_period, 'dd/mm/yyyy') period_for_invoicing
      ,i.due_date due_date
      ,round(i.total_amount_net, billing.getAcctPrecision(nvl(adv.agency_account_id, adv.account_id))) amount_net
  from invoice i
      ,users u
      ,account adv
      ,account ag
      ,accountaddress billing_adr
      ,accountaddress legal_adr
 where i.status = ?
   and adv.internal_account_id = ?
   and i.bill_to_user_id = u.user_id
   and i.adv_account_id = adv.account_id
   and i.sold_to_user_address_id = legal_adr.address_id (+)
   and i.bill_to_user_address_id = billing_adr.address_id (+)
   and adv.agency_account_id = ag.account_id (+)
order by 1
]]></xml-property>
